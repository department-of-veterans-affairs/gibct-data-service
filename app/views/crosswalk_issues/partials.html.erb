<div class="row row-space-6" style="padding-bottom: 10px">
  <div class="col-xs-12">
    <h2>
      Issues
              <%= link_to 'Export', dashboard_export_partials_path,
          class: "btn dashboard-btn-success btn-xs", role: "button" %>
    </h2>
    <table class="table table-hover table-condensed table-bordered table-responsive crosswalk-issues">
      <thead class="system-header">
        <th colspan="1">ArfGiBill</th>
        <th colspan="4">WEAMS</th>
        <th colspan="2">IPEDS HD</th>
        <th colspan="3">Crosswalk</th>
      </thead>
      <thead class="sub">
        <th># GI Bill Students</th>
        <th>Institution Name</th>
        <th>Facility Code</th>
        <th>IPEDS</th>
        <th>OPE</th>
        <th>IPEDS</th>
        <th>OPE</th>
        <th>IPEDS</th>
        <th>OPE</th>
        <th>Action</th>
      </thead>
      <tbody>
      <%= render partial: 'partial_issues', collection: @issues, as: :issue %>
      </tbody>
    </table>
  </div>
</div>
