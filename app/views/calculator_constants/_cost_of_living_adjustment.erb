<div class="cola-div">
  <%= label_tag "ch#{cola.benefit_type}", "Ch. #{cola.benefit_type} "%></label>
  <input type="number"
         min="0.00"
         step="0.01"
         required
         disabled
         id="ch<%= cola.benefit_type %>"
         name="cost_of_living_adjustments[<%= cola.id %>][rate]"
         data-colas-form-target="input"
         value="<%= cola.rate %>" />
  <span>%</span>
</div>
